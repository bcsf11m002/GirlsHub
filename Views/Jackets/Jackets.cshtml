<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Frameset//EN"
          "http://www.w3.org/TR/html4/frameset.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Jackets</title>
    <link href="Content/bootstrap.min.css" rel="stylesheet" />
    @Scripts.Render("~/bundles/jquery")
    <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.4/jquery.min.js"></script>

    <script type="text/javascript" src="/fancybox/jquery.fancybox-1.3.4.pack.js"></script>
    <script type="text/javascript" src="/fancybox/jquery.easing-1.4.pack.js"></script>
    <link rel="stylesheet" href="/fancybox/jquery.fancybox-1.3.4.css" type="text/css" media="screen" />
    <script>
        $(document).ready(function () {

            /* This is basic - uses default settings */

            $("a#single_image").fancybox();

            /* Apply fancybox to multiple items */

            $("a.group").fancybox({
                'transitionIn': 'elastic',
                'transitionOut': 'elastic',
                'speedIn': 600,
                'speedOut': 200,
                'overlayShow': false
            });

        });
    </script>

</head>
<body id="body1">
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li style="left:-15px;"><a href="/home">Home</a></li>
                    <li style="left:-10px;"><a href="/home/About">About</a></li>
                    <li style="left:-5px;"><a href="/home/Contact">Contact</a></li>
                    <li style="left:759px;"><a href="/account/Register">Register</a></li>
                    <li style="left:759px;"><a href="/account/Login">Log in</a></li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container" id="d1">
        <div class="row">
            <img src="/jackets/Jimage.png" class="col-md-2" />
            <div class="col-md-10">
                <center><h1 style="font-size:100px;font-family:'Comic Sans MS';background-color:lightgray; margin-top:-85px; padding-bottom:0.37em;padding-top:0.16em;margin-left:14px;width:920px;padding-left:2.8em;">Jackets</h1></center>
            </div>
        </div>
        <div class="row">
            <ul class="col-md-2" style="list-style-type:none;font-family:'Comic Sans MS';font-size:large;">
                <li style="padding-top: 50px;">@Html.ActionLink("Jackets", "Jackets", "Jackets")</li>
                <li style="padding-top: 30px;">@Html.ActionLink("Clothes", "Clothes", "Clothes")</li>
                <li style="padding-top: 30px;">@Html.ActionLink("Watches", "Watches", "Watches")</li>
                <li style="padding-top: 30px;">@Html.ActionLink("Coats", "Coats", "Coats")</li>
                <li style="padding-top: 30px;">@Html.ActionLink("Sweaters", "Sweaters", "Sweaters")</li>
                <li style="padding-top: 30px;">@Html.ActionLink("Eastern Dresses", "EasternDresses", "EasternDresses")</li>
                <li style="padding-top: 30px;">@Html.ActionLink("Accessories", "Accessories", "Accessories")</li>
            </ul>
            <div id="d1" class="col-md-10" style="top:50px;">
                <div id="myCarousel" class="carousel slide" data-ride="carousel">
                    <!-- Indicators -->
                    <ol class="carousel-indicators">
                        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                        <li data-target="#myCarousel" data-slide-to="1"></li>
                        <li data-target="#myCarousel" data-slide-to="2"></li>
                    </ol>
                    <!-- Wrapper for slides -->
                    <div class="carousel-inner" role="listbox" style="align-content:center;">
                        <div class=" item active">
                            <img src="/jackets/Jacket1.png" style="margin: 0 auto; width: auto; height: 350px; max-height: 350px;">
                        </div>
                        <div class="item">
                            <img src="/jackets/Jacket2.png" style="margin: 0 auto; width: auto; height: 350px; max-height: 350px;">
                        </div>
                        <div class="item">
                            <img src="/jackets/Jacket3.png" style="margin: 0 auto; width: auto; height: 350px; max-height: 350px;">
                        </div>

                    </div>
                    <!-- Left and right controls -->
                    <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
                        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
                        <span class="sr-only">Previous</span>
                    </a>
                    <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
                        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
                        <span class="sr-only">Next</span>
                    </a>
                </div>
                <div class="row">

                    <a id="single_image" href="/jackets/J1">
                        <img src="/jackets/J1.png" class="col-md-3" style="top: 50px; left: 130px; height: 390px;" />
                    </a>
                    <a href="/jackets/J2">
                        <img src="/jackets/J2.png" class="col-md-3" style="top: 50px; left: 130px; height: 390px;" />
                    </a>
                    <a href="/jackets/J3">
                        <img src="/jackets/J3.png" class="col-md-3" style="top: 50px; left: 130px; height: 390px;" />
                    </a>
                </div>
                <div class="row">
                    <a href="/jackets/J4">
                        <img src="/jackets/J4.png" class="col-md-3" style="top: 100px; left: 130px; height: 390px; margin-bottom: 50px;" />
                    </a>
                    <a href="/jackets/J5">
                        <img src="/jackets/J5.png" class="col-md-3" style="top: 100px; left: 130px; height: 390px; margin-bottom: 50px;" />
                    </a>
                    <a href="/jackets/J6">
                        <img src="/jackets/J6.png" class="col-md-3" style="top: 100px; left: 130px; height: 390px; margin-bottom: 50px;" />
                    </a>
                </div>
            </div>
        </div>
    </div>
    <script src="Scripts/jquery-1.10.2.min.js"></script>
    <script src="Scripts/bootstrap.min.js"></script>

</body>
</html>

